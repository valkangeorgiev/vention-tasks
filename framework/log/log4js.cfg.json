
{
  "appenders": {
    "out": { "type": "console" },
    "app": { "type": "file", "filename":  "logs/logs.log"},
    "layout": { "type": "pattern", "pattern": "%[%d{yyyy-MM-/dd hh:mm:ss} %p %n%]" } 
  },
  "categories": {
    "default": { "appenders": ["out", "app"], "level": "debug" },
    "trace": { "appenders": ["app"], "level": "trace" }
  }
}